    display: block;
    width: 100%;
    height: 100%;
    border-radius: 3rpx;
    background: lightblue;
    animation: changePosition-transform-150 4s linear infinite;
}

@keyframes changePosition-transform-150 {
    0% {
        transform: translate(-150rpx);
    }

    50% {
        transform: translate(0);
    }

    100% {
        transform: translate(150rpx);
    }
}

