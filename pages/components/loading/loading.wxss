.loading-9 {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    border: 5rpx solid lightblue;
    animation: turn-0-360 2s linear infinite;
}

.loading-9 .loading-item {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    background: lightblue;
    position: absolute;
    left: 50%;
    margin-top: -15rpx;
    margin-left: -15rpx;
    animation: changeBgColor-lightblue-lightblue 2s linear infinite;
}

@keyframes changeBgColor-lightblue-lightblue {
    0% {
        background: lightblue;
    }

    100% {
        background: lightblue;
    }
}

@keyframes turn-0-360 {
    0% {
        transform: rotate(0deg);
        border-color: lightblue;
    }

    100% {
        transform: rotate(360deg);
        border-color: lightblue;
    }
}

