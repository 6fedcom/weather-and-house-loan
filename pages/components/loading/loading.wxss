.loading-global {
    width: 100vw;
    display: flex;
    min-height: 100vh;
    margin: 0;
    position: relative;
}

.loading-container {
    margin: auto;
    position: relative;
    /* position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%); */
}

.loading-1 {
    width: 70rpx;
    display: flex;
    justify-content: space-around;
}

.loading-1 .loading-item {
    display: inline-block;
    width: 8rpx;
    height: 100%;
    border-radius: 4rpx;
    background: lightblue;
    animation: load1 2s ease infinite;
}

@keyframes load1 {

    0%,
    100% {
        height: 40rpx;
        background: lightblue;
    }

    50% {
        height: 70rpx;
        margin: -15rpx 0;
        background: lightblue;
    }
}

.loading-1 .loading-item:nth-child(2) {
    animation-delay: 0.2s;
}

.loading-1 .loading-item:nth-child(3) {
    animation-delay: 0.4s;
}

.loading-1 .loading-item:nth-child(4) {
    animation-delay: 0.6s;
}

.loading-1 .loading-item:nth-child(5) {
    animation-delay: 0.8s;
}

.loading-2 {
    width: 150rpx;
    height: 4rpx;
    border-radius: 2rpx;
    position: relative;
    background: lightblue;
    animation: changeBgColor-lightblue-lightblue 1.04s ease-in infinite alternate;
}

.loading-2 .loading-item {
    display: inline-block;
    width: 16px;
    height: 16px;
    border-radius: 50%;
    background: lightblue;
    position: absolute;
    margin-top: -12rpx;
    margin-left: -8rpx;
    animation: changePosition-lightblue-lightblue 1.04s ease-in infinite alternate;
}

@keyframes changeBgColor-lightblue-lightblue {
    0% {
        background: lightblue;
    }

    100% {
        background: lightblue;
    }
}

@keyframes changePosition-lightblue-lightblue {
    0% {
        background: lightblue;
    }

    100% {
        margin-left: 142rpx;
        background: lightblue;
    }
}

